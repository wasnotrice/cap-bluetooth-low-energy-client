{
  "name": "cap-bluetooth-low-energy-client",
  "version": "1.0.0",
  "description": "Capacitor Bluetooth Low Energy Client Plugin",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "tsc",
    "postbuild": "cp src/utils/*.js dist/esm/utils/",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "robmarti",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "@types/web-bluetooth": "0.0.4",
    "typescript": "^3.2.4"
  },
  "files": [
    "LICENSE",
    "dist/",
    "ios/Plugin*",
    "ios/Podfile",
    "android/*.iml",
    "android/*.gradle",
    "android/gradle*",
    "android/*.properties",
    "android/*.pro",
    "android/src",
    "CapBluetoothLowEnergyClient.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robmarti/cap-bluetooth-low-energy-client.git"
  },
  "bugs": {
    "url": "https://github.com/robmarti/cap-bluetooth-low-energy-client.git/issues"
  }
}
